class Point {
    let pub x: Int;

    let pub y: Int;

    fn pub Point(x: Int, y: Int) {
        this.x = x
        this.y = y
    }

    fn pub add(other: Point) {
        x += other.x;
        y += other.y;
    }

    fn pub toString: String {
        let str = String()
        str += "("
        str += x.toString
        str += ", "
        str += y.toString
        str += ")"
        ret str
    }
}

fn pub main {
    let p1 = Point(5, 5)
    let p2 = Point(20, 20)
    p1.add(p2)
}
