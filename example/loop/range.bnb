fn main {
  // Compact one liner
  // TODO range c := from 0 ... 10 step 2  => print c
  for c in 0 .. 10 => print(c)

  // You can also use braces.
  for c in 0 .. 10 {
    var d = c * 2
    print(d)
  }

  // Obtain index
  for c, i in 1 ... 10 => print('%i %c')

  // Add a step. Default is 1.
  for i in 1 .. 10, 2 => print i
}
