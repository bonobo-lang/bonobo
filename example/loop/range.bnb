fn pub main {
  // Compact one liner
  // TODO range c := from 0 ... 10 step 2  => print c
  range c from 0 to 10  => print c

  // Obtain index
  range c, i from 1 to 10 => print i.toString + ' ' + c.toString

  range i from 1 to 10 step 2 {
    print i
  }

  // Dont need c?
  range from 1 to 10 => print 'Bonobo'
}
